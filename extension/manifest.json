{
    "manifest_version": 3,
    "name": "Cortex Capture",
    "description": "Quickly save text to your Cortex knowledge base",
    "version": "1.0.0",
    "permissions": [
        "contextMenus"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}